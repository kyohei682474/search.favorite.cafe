<h1>カフェ登録</h1>

<%= form_with(model: @cafe, url: cafes_path, local: true) do |form| %>

  <div class="field">
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>

  <div class="field">
    <%= form.label :address %>
    <%= form.text_field :address %>
  </div>

  <div class="field">
    <%= form.label :business_hours_hours %>
    <%= form.text_field :opening_hours %>
  </div>


  <div class="actions">
    <%= form.submit "登録する" %>
  </div>
<% end %>